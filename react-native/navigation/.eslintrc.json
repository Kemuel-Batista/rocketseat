{
  "extends": "@rocketseat/eslint-config/react",
  "plugins": [
    "simple-import-sort"
  ],
  "rules": {
    "react-hooks/exhaustive-deps": "off",
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}